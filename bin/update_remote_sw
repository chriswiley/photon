#!/bin/sh
gamehost=np.h.nofocus.org
identity=/usr/photon/game/.ssh/identity
rsync -e "ssh -i $identity" -a -v --exclude ".git" --exclude "old" remote@$gamehost:/usr/photon/. /usr/photon/.
rsync -e "ssh -i $identity" -a -v --exclude ".git" --exclude "old" remote@$gamehost:/var/photon/game.conf /var/photon/game.conf
